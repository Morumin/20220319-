
import win32com.client

load_file = 'C:\\Users\\IMF\\Desktop\\Python\\李さん転記作業用\\転記する内容のデータ.xlsx'
copy_file = 'C:\\Users\\IMF\\Desktop\\Python\\李さん転記作業用\\転記先のファイル_画像あり_これを複製する - コピー.xlsm'

app = win32com.client.Dispatch("Excel.Application")
app.Visible =  1 
load_wb = app.Workbooks.Open(load_file)
copy_wb = app.Workbooks.Open(copy_file) 

load_sheet = load_wb.Worksheets(1)
copy_sheet = copy_wb.Worksheets(1)

load_sheet.Activate()
copy_sheet.Activate()

copy_sheet.Range("S3").Value = load_sheet.Range("B9").Value

app.DisplayAlerts = False 

copy_wb.Save()

copy_wb.Close()
load_wb.Close(SaveChanges = False)

app.Quit()
