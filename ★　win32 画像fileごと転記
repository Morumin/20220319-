import win32com.client
import os


load_file = 'C:\\\\転記する内容のデータ.xlsx'
copy_file = 'C:\\\\転記先のファイル_画像あり_これを複製する - コピー.xlsx'

app = win32com.client.Dispatch("Excel.Application")
app.Visible =  1 
load_wb = app.Workbooks.Open(load_file)
copy_wb = app.Workbooks.Open(copy_file) 

load_sheet = load_wb.Worksheets(1)
copy_sheet = copy_wb.Worksheets(1)

load_sheet.Activate()
copy_sheet.Activate()


copy_sheet.Cells(3,19).Value =  '零点高TP   ' +str(load_sheet.Cells(9,2).Value)+'m'

app.DisplayAlerts = False 

new_wb = app.Workbooks.Add()
new_sheet = new_wb.Worksheets.Add()
new_sheet.Activate()


copy_sheet.Range("A1:S27").Copy(Destination=new_sheet.Range("A1:S27"))


new_wb.SaveAs(f"{os.getcwd()}\\outputSaveAs.xlsx")

new_wb.Close()
copy_wb.Close()
load_wb.Close(SaveChanges = False)

app.Quit()
